{
  "name": "kliksutin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^5.0.0",
    "@hookform/resolvers": "^3.0.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@sentry/browser": "^8.0.0",
    "@sentry/node": "^8.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/node-cron": "^3.0.8",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@uiw/react-md-editor": "^3.23.3",
    "axios": "^1.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "i18next": "^23.0.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "notistack": "^3.0.1",
    "openai": "^4.47.1",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-i18next": "^14.0.0",
    "react-markdown": "^9.0.0",
    "react-router-dom": "^6.8.1",
    "remark-breaks": "^4.0.0",
    "sequelize": "^6.28.0",
    "umzug": "^3.2.1",
    "unfuck-utf8-headers-middleware": "^1.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.8.2",
    "winston-gelf-transporter": "^1.0.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.0.0",
    "cypress": "^13.0.0",
    "eslint": "^9.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^5.3.4"
  },
  "scripts": {
    "start": "docker compose up",
    "start:prod": "NODE_ENV=production node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/server/index.ts",
    "start:test": "NODE_ENV=test node --experimental-specifier-resolution=node --loader ts-node/esm src/server/index.ts",
    "start:dev": "NODE_ENV=development concurrently \"npm run start:dev:server\" \"npm run start:dev:front\"",
    "start:dev:front": "vite",
    "start:dev:server": "nodemon --exec node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/server/index.ts",
    "test": "npm run test:setup && npm run test:e2e",
    "test:setup": "docker-compose down && docker-compose -f docker-compose.ci.yml rm -vf && docker-compose -f docker-compose.ci.yml up -d --build",
    "test:e2e": "cypress run",
    "test:cypress": "cypress open",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write '**/*.{ts,tsx,json,css,md}'",
    "build": "DISABLE_ESLINT_PLUGIN=true vite build",
    "prepare": "husky"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript",
      "prettier",
      "plugin:cypress/recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "react-hooks",
      "prettier",
      "cypress"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-shadow": "off",
      "@typescript-eslint/no-explicit-any": "warn",
      "react/require-default-props": "off",
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": true
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "import/no-cycle": "off",
      "import/extensions": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 80,
    "jsxSingleQuote": true
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint",
    "**/*.{ts,tsx,json,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000"
}
